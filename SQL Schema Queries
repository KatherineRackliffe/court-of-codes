To create the database:

CREATE TABLE Book
(
  ISBN VARCHAR NOT NULL,
  BookTitle VARCHAR NOT NULL,
  AuthorFName VARCHAR NOT NULL,
  AuthorLName VARCHAR NOT NULL,
  DatePublished DATE NOT NULL,
  PageCount INT NOT NULL,
  PRIMARY KEY (ISBN)
);
CREATE TABLE User
(
  UserID SERIAL NOT NULL,
  EmailAddress VARCHAR NOT NULL,
  PasswordHash INT NOT NULL,
  Username VARCHAR NOT NULL,
  Salt  NOT NULL,
  PRIMARY KEY (UserID)
);
CREATE TABLE UserList
(
  ListID SERIAL NOT NULL,
  ListName VARCHAR NOT NULL,
  UserID INT NOT NULL,
  PRIMARY KEY (ListID),
  FOREIGN KEY (UserID) REFERENCES User(UserID) ON DELETE CASCADE ON UPDATE CASCADE
);
CREATE TABLE BookInList
(
  ListID INT NOT NULL,
  ISBN INT NOT NULL,
  PRIMARY KEY (ListID, ISBN),
  FOREIGN KEY (ListID) REFERENCES UserList(ListID) ON DELETE CASCADE ON UPDATE CASCADE,
  FOREIGN KEY (ISBN) REFERENCES Book(ISBN) ON DELETE CASCADE ON UPDATE CASCADE,
  UNIQUE (ListID, ISBN)
);
CREATE TABLE UserTag
(
  TagName INT NOT NULL,
  UserID INT NOT NULL,
  ISBN INT NOT NULL,
  FOREIGN KEY (UserID) REFERENCES User(UserID) ON DELETE CASCADE ON UPDATE CASCADE,
  FOREIGN KEY (ISBN) REFERENCES Book(ISBN) ON DELETE CASCADE ON UPDATE CASCADE,
  UNIQUE (TagName),
  UNIQUE (TagName, ISBN, UserID)
);
CREATE TABLE UserReview
(
  NumericalReview INT NOT NULL,
  UserID INT NOT NULL,
  ISBN INT NOT NULL,
  PRIMARY KEY (UserID, ISBN),
  FOREIGN KEY (UserID) REFERENCES User(UserID) ON DELETE CASCADE ON UPDATE CASCADE,
  FOREIGN KEY (ISBN) REFERENCES Book(ISBN) ON DELETE CASCADE ON UPDATE CASCADE,
  UNIQUE (UserID, ISBN)
); 
